<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <!-- Use the actual MixMate transparent logo -->
    <!-- Since we can't directly embed a PNG in a vector, we'll create a simple reference -->
    <!-- The logo will be centered and sized appropriately for the icon -->
    <group
        android:translateX="27"
        android:translateY="27"
        android:scaleX="0.5"
        android:scaleY="0.5">
        
        <!-- Placeholder for the actual logo - this would normally be replaced -->
        <!-- with the actual MixMate logo converted to vector paths -->
        <!-- For now, we'll use a simplified version that represents the logo -->
        <path
            android:fillColor="@color/primary_color"
            android:pathData="M54,54m-54,0a54,54 0,1 1,108 0a54,54 0,1 1,-108 0" />
            
        <!-- MixMate Text/Logo representation -->
        <group android:translateX="20" android:translateY="35">
            <!-- M -->
            <path android:fillColor="#FFFFFF"
                android:pathData="M0,0 L0,20 L5,20 L5,8 L10,18 L15,8 L15,20 L20,20 L20,0 L12,0 L7.5,12 L3,0 Z" />
            <!-- i -->
            <path android:fillColor="#FFFFFF"
                android:pathData="M25,8 L25,20 L30,20 L30,8 Z" />
            <path android:fillColor="#FFFFFF"
                android:pathData="M25,3 L25,6 L30,6 L30,3 Z" />
            <!-- x -->
            <path android:fillColor="#FFFFFF"
                android:pathData="M35,8 L40,14 L45,8 L50,8 L43,16 L50,24 L45,24 L40,18 L35,24 L30,24 L37,16 L30,8 Z" />
        </group>
        
        <!-- Mate text would go below, simplified for icon -->
        <group android:translateX="25" android:translateY="55">
            <path android:fillColor="#FFFFFF"
                android:pathData="M0,0 L0,12 L5,12 L5,5 L10,12 L15,5 L15,12 L20,12 L20,0 L12,0 L7.5,8 L3,0 Z" />
        </group>
            
    </group>
        
</vector>
